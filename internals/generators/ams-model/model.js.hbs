{{#if wantActionsAndReducer}}
import injectReducerModel from 'utils/injectReducerModel';
{{/if}}
{{#if wantSaga}}
import injectSagaModel from 'utils/injectSagaModel';
{{/if}}

{{#if wantActionsAndReducer}}
import reducer from './reducer';
{{/if}}
{{#if wantSaga}}
import saga from './saga';
{{/if}}

const loadModel = (store) => {
  {{#if wantActionsAndReducer}}
  injectReducerModel('{{ camelCase name }}', reducer, store);
  {{/if}}
  {{#if wantSaga}}
  injectSagaModel('{{ camelCase name }}', saga, store);
  {{/if}}
};
