{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "./cypress",
    "lib": [
      "ES2017",
      "dom"
    ],
    "types": [
      "cypress",
      "./cypress/support"
    ],
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "include": [
    "./cypress/**/*.ts",
    "./cypress/**/*.d.ts",
  ]
}