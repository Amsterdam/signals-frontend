{
  "name": "sia",
  "version": "1.0.0",
  "description": "Signalen Informatievoorziening Amsterdam based on react-boilerplate ",
  "repository": {
    "type": "git",
    "url": "https://github.com/Amsterdam/signals-frontend.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=10"
  },
  "author": "Datapunt Amsterdam",
  "license": "MPL-2.0",
  "scripts": {
    "analyze": "node ./internals/scripts/analyze.js",
    "build:acc": "cross-env NODE_ENV=acceptance webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build:clean": "rimraf ./build",
    "build:prod": "cross-env NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color -p --progress --hide-modules --display-optimization-bailout",
    "build": "npm run build:prod",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "dyson:sample": "nodemon --watch test/mock/sample --exec babel-node --presets=latest ./test/mock/sample",
    "dyson:server": "nodemon --watch test/mock/api --exec babel-node --presets=latest test/mock/api",
    "extract-intl": "babel-node --presets env,stage-0 -- ./internals/scripts/extract-intl.js",
    "generate": "plop --plopfile internals/generators/index.js",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:staged": "lint-staged",
    "lint": "npm run lint:js",
    "npmcheckversion": "node ./internals/scripts/npmcheckversion.js",
    "prebuild": "npm run build:clean",
    "preinstall": "npm run npmcheckversion",
    "pretest": "npm run test:clean && npm run lint",
    "start:dev": "npm-run-all -p dyson:server start:proxy-dev",
    "start:prod": "cross-env NODE_ENV=production node server",
    "start:production": "npm run test && npm run build && npm run start:prod",
    "start:proxy-dev": "cross-env NODE_ENV=development node server -- --proxyConfig=proxy.conf.dev.js --port=3001",
    "start:tunnel": "cross-env NODE_ENV=development ENABLE_TUNNEL=true node server",
    "start": "cross-env NODE_ENV=development node server -- --proxyConfig=proxy.conf.api.js --port=3001",
    "test:clean": "rimraf ./coverage",
    "test:no-coverage": "cross-env NODE_ENV=test jest ",
    "test:only": "cross-env NODE_ENV=test jest --watch --onlyChanged --coverage --verbose false",
    "test:update": "cross-env NODE_ENV=test jest --updateSnapshot --coverage --verbose false",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll --coverage --verbose false",
    "test": "cross-env NODE_ENV=development jest --coverage"
  },
  "lint-staged": {
    "*.js": "lint:eslint"
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.test.{js,jsx}",
      "!src/test/**/*.{js,jsx}",
      "!src/*/RbGenerated*/*.{js,jsx}",
      "!src/app.js",
      "!src/global-styles.js",
      "!src/*/*/Loadable.{js,jsx}",
      "!src/**/definitions/*"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 84,
        "branches": 83,
        "functions": 80,
        "lines": 84
      }
    },
    "coverageReporters": [
      "lcov"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/internals/"
    ],
    "moduleNameMapper": {
      ".*\\.(css|less|styl|scss|sass)$": "<rootDir>/internals/mocks/cssModule.js",
      ".*\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/internals/mocks/image.js"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/internals/testing/test-bundler.js",
    "testRegex": ".*\\.test\\.js$",
    "testEnvironment": "./internals/testing/jest-environment-jsdom-global-fix",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "7.4.3",
    "@datapunt/leaflet-geojson-bbox-layer": "0.1.1",
    "@sentry/browser": "^5.4.3",
    "amsterdam-amaps": "^2.0.0",
    "amsterdam-stijl": "^3.0.2",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "connected-react-router": "^6.5.2",
    "history": "^4.9.0",
    "hoist-non-react-statics": "^3.3.0",
    "immer": "^3.1.3",
    "intl": "1.2.5",
    "leaflet": "1.0.3",
    "leaflet-headless": "^0.2.6",
    "lodash.defer": "^4.1.0",
    "lodash.every": "^4.6.0",
    "lodash.foreach": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.isarray": "^4.0.0",
    "lodash.isboolean": "^3.0.3",
    "lodash.isequal": "^4.5.0",
    "lodash.isfunction": "^3.0.9",
    "lodash.isobject": "^3.0.2",
    "lodash.isstring": "^4.0.1",
    "lodash.map": "^4.6.0",
    "lodash.orderby": "^4.6.0",
    "lodash.pullallby": "^4.7.0",
    "lodash.set": "^4.3.2",
    "lodash.some": "^4.6.0",
    "lodash.sortby": "^4.7.0",
    "moment": "^2.24.0",
    "proj4": "^2.4.4",
    "react": "^16.8.6",
    "react-albus": "^2.0.0",
    "react-datepicker": "1.5.0",
    "react-dom": "^16.8.6",
    "react-intl": "^2.9.0",
    "react-loadable": "4.0.3",
    "react-reactive-form": "1.0.27",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "recharts": "^1.4.2",
    "redux": "^4.0.4",
    "redux-react-hook": "^3.3.2",
    "redux-saga": "^1.0.5",
    "reselect": "^4.0.0",
    "styled-components": "^4.3.2",
    "url-polyfill": "^1.1.7",
    "warning": "3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-transform-react-constant-elements": "^7.5.0",
    "@babel/plugin-transform-react-inline-elements": "7.2.0",
    "@babel/preset-env": "7.4.3",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.4.0",
    "add-asset-html-webpack-plugin": "^2.1.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-dynamic-import-node": "2.2.0",
    "babel-plugin-inline-react-svg": "^1.1.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-react-intl": "^4.1.0",
    "babel-plugin-styled-components": "1.10.0",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "chalk": "1.1.3",
    "circular-dependency-plugin": "3.0.0",
    "compression-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "coveralls": "2.13.1",
    "cross-env": "^5.2.0",
    "css-loader": "^3.1.0",
    "dyson": "^2.0.1",
    "dyson-generators": "^0.2.0",
    "dyson-image": "^0.2.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.1.4",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.18.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "eslint-plugin-redux-saga": "^1.0.0",
    "eventsource-polyfill": "0.9.6",
    "exports-loader": "^0.7.0",
    "express": "^4.16.3",
    "file-loader": "^4.1.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-proxy-middleware": "^0.18.0",
    "image-webpack-loader": "^5.0.0",
    "imports-loader": "^0.8.0",
    "invariant": "^2.2.4",
