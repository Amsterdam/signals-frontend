    "analyze:clean": "rimraf stats.json",
    "analyze": "BABEL_ENV=production NODE_ENV=production node ./internals/scripts/analyze.js",
    "build:clean": "rimraf ./build",
    "build": "cross-env BABEL_ENV=production NODE_ENV=production webpack --config internals/webpack/webpack.prod.babel.js --color --mode production --progress",
    "build:netlify": "cross-env BABEL_ENV=production NODE_ENV=netlify webpack --config internals/webpack/webpack.prod.babel.js --color --mode production --progress",
    "clean:all": "npm run analyze:clean && npm run test:clean && npm run build:clean",
    "clean": "shjs ./internals/scripts/clean.js",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "format": "prettier --write .",
    "lint:eslint": "NODE_ENV=production eslint",
    "lint:js": "npm run lint:eslint -- . ",
    "lint:verbose": "npm run lint:eslint -- --format=mo . ",
    "lint:fix": "npm run lint:js -- --fix",
    "lint:nibble": "NODE_ENV=production eslint-nibble --cache --ext=js,jsx,ts,tsx internals server src",
    "lint:find-rules": "eslint-find-rules -dun .eslintrc.json",
    "lint": "npm run lint:js",
    "preanalyze": "npm run analyze:clean",
    "prebuild": "npm run build:clean",
    "pretest": "npm run test:clean",
    "start:prod": "cross-env BABEL_ENV=production NODE_ENV=production node server",
    "start:tunnel": "cross-env BABEL_ENV=production NODE_ENV=production ENABLE_TUNNEL=true node server",
    "start": "cross-env NODE_ENV=development BABEL_ENV=development node server -- --port=3001",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=--unhandled-rejections=warn jest --coverage --maxWorkers=75%",
    "validate-config": "node ./internals/scripts/validate-config.js",
    "validate-base-config": "node ./internals/scripts/validate-base-config.js"
  },
